{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "cybox-container",
  "description": "STIX gateway to Cybox Objects",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "description": "The type of this object, which MUST be the literal `cybox-container`"
    },
    "objects": {
      "type": "array",
      "description": "List of CybOX objects",
      "items": {"$ref": "../cybox/object.json"}
    },
    "actions": {
      "type": "array",
      "description": "List of CybOX actions",
      "items": {"$ref": "../cybox/action.json"}
    },
    "relationships": {
      "type": "array",
      "description": "List of CybOX relationships",
      "items": {"$ref": "../cybox/relationship.json"}
    }
  },
  "required": ["type"]
}
